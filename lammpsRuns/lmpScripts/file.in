units		metal
atom_style      atomic
boundary        p p p

variable        a equal 3.520
lattice	fcc $a 

region          mybox block -15 15 -15 15 -10 10 
create_box      3 mybox

create_atoms	1 region mybox
mass            1  26.98
mass            2  26.98
mass            3  26.98

run 0

write_data initial.data

